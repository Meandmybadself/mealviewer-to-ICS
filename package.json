{
  "name": "menucalendar",
  "version": "1.0.0",
  "engines": {
    "node": ">= 20"
  },
  "description": "An ICS web service that accepts a school ID & returns an ICS calendar document from the Mealviewer.com API",
  "type": "module",
  "targets": {
    "main": {
      "context": "node",
      "includeNodeModules": [
        "ical-generator"
      ]
    }
  },
  "scripts": {
    "build": "parcel build src/index.ts --target main"
  },
  "author": "Jeffery Bennett <me@meandmybadself.com>",
  "license": "ISC",
  "dependencies": {
    "@cloudflare/workers-types": "^4.20240320.1",
    "@types/mime": "^2.0.3",
    "ical-generator": "^6.0.1",
    "types-cloudflare-worker": "^1.2.0"
  },
  "devDependencies": {
    "@cloudflare/kv-asset-handler": "^0.2.0",
    "@types/node": "^16.18.91",
    "nodemon": "^3.1.0",
    "parcel": "^2.12.0",
    "ts-node": "^10.9.2",
    "typescript": "^4.9.5",
    "wrangler": "^3.37.0"
  }
}